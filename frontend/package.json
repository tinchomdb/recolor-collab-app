{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "npm run typecheck",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css}\"",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "quality": "npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "vue": "^3.5.25",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.5"
  }
}
